{
	"name": "panchobot",
	"version": "1.0.0",
	"description": "A peronal bot made by Pancho with special tournament modifications.",
	"main": "/src/main.ts",
	"scripts": {
		"start": "esbuild src/main.ts --bundle --platform=node --packages=external --outfile=app.js && node app.js",
		"dev": "tsx watch src/main.ts",
		"build": "esbuild src/main.ts --bundle --platform=node --packages=external --outfile=app.js",
		"package": "esbuild src/main.ts --bundle --platform=node --outfile=app.js"
	},
	"keywords": [
		"discord",
		"bot",
		"typescript",
		"esbuild",
		"mysql",
		"mariadb",
		"typeorm",
		"osu!"
	],
	"author": "Francisco Hauva <phauva@gmail.com>",
	"license": "MIT",
