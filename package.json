		"@paralleldrive/cuid2": "^2.2.1",
		"discord.js": "^14.11.0",
		"dotenv": "^16.3.1",
		"esbuild": "^0.17.19",
		"mysql2": "^3.4.0",
		"osu-api-extended": "^2.7.51",
		"prettier": "^2.8.8",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.3.17",
		"typescript": "^5.1.3",
		"winston": "^3.9.0",
		"winston-daily-rotate-file": "^4.7.1"
	},
