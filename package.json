{
	"name": "panchobot",
	"version": "1.0.0",
	"description": "A peronal bot made by Pancho with special tournament modifications.",
	"main": "/src/main.ts",
	"scripts": {
		"start": "ts-node src/main.ts",
		"start:prod": "npx prisma migrate deploy && ts-node src/main.ts",
		"dev": "cross-env NODE_ENV=development nodemon src/main.ts",
		"lint": "eslint .  --ext .ts",
		"lint:fix": "eslint .  --ext .ts --fix"
	},
	"keywords": [
		"discord",
		"bot",
		"typescript",
		"esbuild",
		"mysql",
		"mariadb",
		"typeorm",
		"osu!"
	],
	"author": "Francisco Hauva <phauva@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.382.0",
		"@aws-sdk/s3-request-presigner": "^3.382.0",
		"@axiomhq/winston": "^0.1.3",
		"@paralleldrive/cuid2": "^2.2.1",
		"@prisma/client": "5.1.0",
		"adm-zip": "^0.5.10",
		"axios": "^1.4.0",
		"discord.js": "^14.12.1",
		"dotenv": "^16.3.1",
		"luxon": "^3.3.0",
		"mysql2": "^3.5.2",
		"osu-api-extended": "^2.7.65",
		"prettier": "^2.8.8",
		"typescript": "^5.1.6",
		"winston": "^3.10.0",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@types/adm-zip": "^0.5.0",
		"@types/luxon": "^3.3.1",
		"@types/node": "^20.4.5",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.0.1",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
		"eslint-plugin-promise": "^6.0.0",
		"nodemon": "^3.0.1",
		"prisma": "^5.1.0",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 2
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"@typescript-eslint"
		],
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/eslint-recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"ignorePatterns": [
			"dist",
			"node_modules"
		]
	}
}
