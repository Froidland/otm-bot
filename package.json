{
	"name": "panchobot",
	"version": "1.0.0",
	"description": "A peronal bot made by Pancho with special tournament modifications.",
	"main": "/src/main.ts",
	"scripts": {
		"start": "esbuild src/main.ts --bundle --platform=node --packages=external --outfile=app.js && node app.js",
		"dev": "tsx watch src/main.ts",
		"build": "esbuild src/main.ts --bundle --platform=node --packages=external --outfile=app.js",
		"package": "esbuild src/main.ts --bundle --platform=node --outfile=app.js"
	},
	"type": "module",
	"keywords": [
		"discord",
		"bot",
		"typescript",
		"esbuild",
		"mysql",
		"mariadb",
		"typeorm",
		"osu!"
	],
	"author": "Francisco Hauva <phauva@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.369.0",
		"@aws-sdk/s3-request-presigner": "^3.369.0",
		"@paralleldrive/cuid2": "^2.2.1",
		"adm-zip": "^0.5.10",
		"axios": "^1.4.0",
		"discord.js": "^14.11.0",
		"dotenv": "^16.3.1",
		"esbuild": "^0.17.19",
		"luxon": "^3.3.0",
		"mysql2": "^3.5.1",
		"osu-api-extended": "^2.7.64",
		"prettier": "^2.8.8",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.3.17",
		"typescript": "^5.1.6",
		"winston": "^3.10.0",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@types/adm-zip": "^0.5.0",
		"@types/luxon": "^3.3.0",
		"@types/node": "^20.4.2",
		"ts-node": "^10.9.1",
		"tsx": "^3.12.7"
	},
	"prettier": {
		"useTabs": true,
		"tabWidth": 2
	}
